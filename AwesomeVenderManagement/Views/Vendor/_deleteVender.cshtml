<modal-dialog show='vendorDeleteData.modalShown'
              width='500px'
              dialog-title='Delete Vendor'
              on-close='logClose()'>

    <div>
        Are you sure, you want to delete this vendor ?
        <span style="margin-left:3%">
            <button id="btnDelete"
                    ng-click="deleteVendor()"
                    class="btn btn-danger">
                Delete
            </button>
        </span>
    </div>

</modal-dialog>
